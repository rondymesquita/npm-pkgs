{
  "name": "@rondymesquita/flow",
  "version": "0.2.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/rondymesquita/shell.git"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./src/index.ts",
  "bin": {
    "nutshell": "./src/bin.ts"
  },
  "files": [
    "src/**/*"
  ],
  "exports": {
    ".": "./src/index.ts"
  },
  "typesVersions": {
    "*": {
      "globals": [
        "./src/globals.ts"
      ],
      "core": [
        "./src/core.ts"
      ]
    }
  },
  "scripts": {
    "test": "vitest --watch=false --coverage",
    "test:watch": "vitest",
    "dev": "tsx --watch ./src/dev.ts --alpha=5 -a=0 --beta=1.0 -b=0.5 --gama=gama -g=gama --delta -d --epsilon=true -e=false",
    "build": "rimraf ./dist && tsc && copyfiles ./example ./dist",
    "release": "release-it --ci",
    "docs": "typedoc"
  },
  "keywords": [],
  "author": "rondymesquita",
  "license": "MIT"
}
